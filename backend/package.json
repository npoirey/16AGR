{
  "name": "16AGR_backend",
  "version": "0.0.1",
  "description": "Node.js on Docker",
  "author": "Nicolas Poirey <nicolas.poirey@gmail.com>",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=app:production node dist/server",
    "build": "./node_modules/babel-cli/bin/babel.js lib -d dist --presets es2015,stage-2",
    "dev": "DEBUG=app:production ./node_modules/nodemon/bin/nodemon.js -L server --exec ./node_modules/babel-cli/bin/babel-node.js",
    "db:bootstrap": "npm run db:migrate && npm run db:seed",
    "db:migrate": "./node_modules/knex-migrate/lib/cli.js up",
    "db:migrate:make": "./node_modules/knex/bin/cli.js migrate:make",
    "db:rollback": "./node_modules/knex-migrate/lib/cli.js rollback",
    "db:seed": "./node_modules/knex/bin/cli.js seed:run",
    "db:seed:make": "./node_modules/knex/bin/cli.js seed:make",
    "db:redo": "./node_modules/knex-migrate/lib/cli.js redo",
    "db:reset": "npm run db:redo && npm run db:seed",
    "lint": "./node_modules/eslint/bin/eslint.js --color --ext .js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --color --ext .js --fix .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha **/*.spec.js*",
    "test:logs": "LOG=true NODE_ENV=test ./node_modules/mocha/bin/_mocha **/*.spec.js*",
    "test:coverage": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- **/*.spec.js*",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/_mocha -t 10000 --watch **/*.spec.js*",
    "test:watch:logs": "LOG=true NODE_ENV=test ./node_modules/mocha/bin/_mocha -t 10000 --watch **/*.spec.js*"
  },
  "dependencies": {
    "ajv": "^4.11.5",
    "babel-register": "^6.24.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bookshelf": "^0.10.3",
    "bookshelf-camelcase": "^1.1.4",
    "bookshelf-cascade-delete": "^2.0.1",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "express-ajv": "^1.0.3",
    "express-jsonschema": "^1.1.6",
    "express-session": "^1.15.1",
    "glob": "^7.1.1",
    "knex": "^0.12.8",
    "knex-migrate": "^1.2.0",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.4",
    "serve-static": "^1.12.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-react": "^6.10.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "node-pre-gyp": "^0.6.33",
    "nodemon": "^1.11.0",
    "sinon": "^2.0.0",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0",
    "supertest-session": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npoirey/16AGR"
  },
  "homepage": "https://github.com/npoirey/16AGR"
}
