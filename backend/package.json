{
  "name": "16AGR_backend",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Nicolas Poirey <nicolas.poirey@gmail.com>",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=app:production node server",
    "dev": "DEBUG=app:production ./node_modules/nodemon/bin/nodemon.js -L server",
    "db:bootstrap": "npm run db:migrate && npm run db:seed",
    "db:migrate": "./node_modules/knex/bin/cli.js migrate:latest",
    "db:migrate:make": "./node_modules/knex/bin/cli.js migrate:make",
    "db:migrate:rollback": "./node_modules/knex/bin/cli.js migrate:rollback",
    "db:seed": "./node_modules/knex/bin/cli.js seed:run",
    "db:seed:make": "./node_modules/knex/bin/cli.js seed:make"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "cookie-parser": "^1.4.3",
    "debug": "^2.3.3",
    "express": "^4.13.3",
    "express-session": "^1.14.2",
    "knex": "^0.12.6",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "serve-favicon": "^2.3.2",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
