{
  "name": "16AGR_backend",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Nicolas Poirey <nicolas.poirey@gmail.com>",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=app:production node server",
    "dev": "DEBUG=app:production ./node_modules/nodemon/bin/nodemon.js -L server",
    "db:bootstrap": "npm run db:migrate && npm run db:seed",
    "db:migrate": "./node_modules/knex-migrate/lib/cli.js up latest",
    "db:migrate:make": "./node_modules/knex/bin/cli.js migrate:make",
    "db:rollback": "./node_modules/knex-migrate/lib/cli.js rollback",
    "db:seed": "./node_modules/knex/bin/cli.js seed:run",
    "db:seed:make": "./node_modules/knex/bin/cli.js seed:make",
    "db:redo": "./node_modules/knex-migrate/lib/cli.js redo",
    "db:reset": "npm run db:redo && npm run db:seed",
    "lint": "./node_modules/eslint/bin/eslint.js --color --ext .js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --color --ext .js --fix ."
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.3.3",
    "express": "^4.13.3",
    "express-session": "^1.14.2",
    "knex": "^0.12.6",
    "knex-migrate": "^1.2.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "serve-static": "^1.11.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^3.0.5",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0"
  }
}
