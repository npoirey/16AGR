{
  "name": "webpack-skel",
  "version": "1.0.0",
  "description": "a simple webpack template",
  "dependencies": {
    "bows": "^1.4.8",
    "lodash": "^3.10.1",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "eslint": "^1.1.0",
    "eslint-config-airbnb": "0.0.7",
    "eslint-plugin-react": "^3.2.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "raw-loader": "~0.5.0",
    "react-hot-loader": "^1.2.8",
    "style-loader": "^0.12.3",
    "url-loader": "~0.5.5",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "lint": "eslint app --ext .js,.jsx",
    "build-dev": "webpack --watch --display-reasons --display-error-details --progress --colors",
    "start": "npm run server",
    "server": "webpack-dev-server  --display-reasons --display-error-details --history-api-fallback --progress --colors -d --hot --inline --host=0.0.0.0 --port 8080",
    "build": "NODE_ENV=production webpack -p --display-reasons --display-error-details --progress --colors --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macropodhq/weback-skel.git"
  },
  "homepage": "https://github.com/macropodhq/webpack-skel"
