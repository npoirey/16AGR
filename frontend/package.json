  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js -p --display-reasons --display-error-details --progress --colors --config webpack.config.js",
    "build-dev": "webpack --watch --display-reasons --display-error-details --progress --colors",
    "lint": "./node_modules/eslint/bin/eslint.js --color --ext .jsx --ext .js src",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --color --ext .jsx --ext .js --fix src",
    "start": "npm run server",
    "server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base src --display-reasons --display-error-details --history-api-fallback --progress -d --hot --inline --host=0.0.0.0 --port 8080",
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --colors --recursive src/**/*.spec.js",
    "test:coverage": "./node_modules/istanbul/lib/cli.js cover --include-all-sources true -x '**/*.spec.js' -x '**/*.min.js' ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --colors --recursive src/**/*.spec.js",
    "test:cov": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --colors --recursive src/**/*.spec.js",
    "test:watch": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --colors --recursive --watch src/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macropodhq/weback-skel.git"
  },
  "homepage": "https://github.com/macropodhq/webpack-skel"
}
