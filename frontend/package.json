{
  "name": "16AGR_frontend",
  "version": "1.0.0",
  "description": "the frontend for 16AGR event planner",
  "main": "webpack.config.js",
  "dependencies": {
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "history": "^1.17.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint app --ext .js,.jsx",
    "build-dev": "webpack --watch --display-reasons --display-error-details --progress --colors",
    "start": "npm run server",
    "server": "./node_modules/.bin/webpack-dev-server --content-base src --display-reasons --display-error-details --history-api-fallback --progress -d --hot --inline --host=0.0.0.0 --port 8080",
    "build": "NODE_ENV=production webpack -p --display-reasons --display-error-details --progress --colors --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macropodhq/weback-skel.git"
  },
  "homepage": "https://github.com/macropodhq/webpack-skel"
}
