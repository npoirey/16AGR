  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js -p --progress --colors --config webpack.config.js",
    "build-dev": "webpack --watch --progress --colors",
    "lint": "./node_modules/eslint/bin/eslint.js --color --ext .jsx --ext .js src",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --color --ext .jsx --ext .js --fix src",
    "start": "npm run server",
    "server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base src --history-api-fallback --progress -d --hot --inline --host=0.0.0.0 --port 8080",
    "test": "if [ $npm_config_pattern ]; then pattern=$npm_config_pattern; echo pattern is SET to: $pattern; else pattern=src/**/*.spec.js*; echo pattern is DEFAULTED to $pattern; fi; ./node_modules/mocha/bin/_mocha $pattern",
    "test:coverage": "if [ $npm_config_pattern ]; then pattern=$npm_config_pattern; echo pattern is SET to: $pattern; else pattern=src/**/*.spec.js*; echo pattern is DEFAULTED to $pattern; fi; ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- $pattern",
    "test:watch": "if [ $npm_config_pattern ]; then echo $npm_config_pattern; pattern=$npm_config_pattern; echo pattern is SET to: $pattern; else pattern=src/**/*.spec.js*; echo pattern is DEFAULTED to $pattern; fi; ./node_modules/mocha/bin/_mocha --watch $pattern"
  },
  "repository": {
    "type": "git",
